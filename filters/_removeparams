    ! ||www.google.*/search?$removeparam=source  # from Ru AdList
    ! hh.ru$removeparam=source
    ! medium.com$removeparam=source  # from NeatURL
    ! sourceforge.net$removeparam=source  # from NeatURL
    ! tiktok.com$removeparam=source  # from NeatURL
