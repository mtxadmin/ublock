    ! ||amazon.*/hz/contact-us/csp$removeparam=source  # from Adguard
    ! cht.com.tw$removeparam=Source  # from Adguard
    ! edx.org$removeparam=source  # from Adguard
    ! ||github.com/signup$removeparam=source  # from Adguard
