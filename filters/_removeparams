$removeparam=/pvid$/
    ! Found independently. Confirmed by months of testing.
    ! aliexpress.com$removeparam=ad_pvid
    ! aliexpress.ru$removeparam=ad_pvid

    ! Found independently. Confirmed by months of testing. Now it is in NeatURL too
    ! aliexpress.com$removeparam=|algo_pvid
    ! aliexpress.ru$removeparam=|algo_pvid
        ! Later: unusual form from Ru AdList
        ! algo_pvid=$document,removeparam=|,domain=aliexpress.*  # from Ru AdList

    ! taobao.com$removeparam=app_pvid  # from Adguard

    ! Found independently. Confirmed by months of testing. 
    ! Update: don't, it breaks search filters in category :-(
    ! Update2. Ok, cleaner urls are more important. Removing again.
    ! Update3. It turns out Adguard has this rule too.
    !aliexpress.com$removeparam=pvid
    !aliexpress.ru$removeparam=pvid
    ! 2022-02: Adguard has this rule also for taobao
    !taobao.com$removeparam=pvid  # from Adguard

# recipientId - do not remove in general. Though it is being used in Oracle Eloqua

$removeparam=ref,domain=~amazon.*  # exclusion is from Adguard.
    ! amazon.*$removeparam=ref  # Found independently. Also is in Adguard
    ! ||data.alexa.com/data/$xmlhttprequest,removeparam=/cdt|ref/  # from Adguard
    ! arcelik.com.tr$removeparam=ref  # from Adguard
    ! buzzfeednews.com$removeparam=ref  # from Adguard
    ! coconala.com$removeparam=ref  # from Adguard
    ! visit.digidip.net$removeparam=/^(ppref|ref|pid)=/
    ! d-markets.net$removeparam=ref  # from Adguard
    ! etsy.com$removeparam=ref  # from NeatURL
    ! facebook.com*/?ref=$removeparam=|,domain=facebook.com  # from Ru AdList
    ! goodreads.com$removeparam=ref  # from Adguard
    ! jasonsavard.com$removeparam=/cUrl|ref/  # from Adguard
    ! video.laxd.com$removeparam=ref  # from Adguard
    ! livedoor.biz$removeparam=ref  # from Adguard
